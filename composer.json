{
    "name": "maurice2k/multicurl",
    "description": "Object based asynchronous multi-curl wrapper",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Moritz Fain",
            "email": "moritz@fain.io"
        }
    ],
    "require": {
        "php": ">=8.0",
        "ext-curl": ">=8.0",
        "ext-json": ">=8.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^12.0"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Maurice\\Multicurl\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Maurice\\Multicurl\\Tests\\": "tests/"
        }
    }
}
